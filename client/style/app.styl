@font-face
  font-family Sofia
  font-style normal
  src url(font/SofiaProLight.otf)

@font-face
  font-family Museo
  font-style normal
  src url(font/museo-sans-rounded__500.woff2)

body, html
  font-family Museo
  padding: 0
  margin: 0
  background-color: #F1F1E9
.title
  font-size: 40px
  color: #212121

.subtitle
  font-size: 30px
  color: #c1c1c1
  margin-top: 50px

.no-margin
  margin: 0px

.no-padding
  padding: 0px

.col
  height: 60px
  display: table
  width: 100%

.text-left
  text-align: left

.text-right
  text-align: right

.separator
  width: 1px
  height: 100%
  background-color: #f1f1e9

@require "variables"
